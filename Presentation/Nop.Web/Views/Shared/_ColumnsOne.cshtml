@{
    Layout = "_Root";
    Html.AppendScriptParts(ResourceLocation.Footer, $"https://pavilion.nop-templates.com/bundles/hozftcleug-vjg9s0ieae1alw1wsf8mbqq_nscgu2r0.min.js");
    Html.AddCssFileParts("https://pavilion.nop-templates.com/bundles/tzjkkql3ws3xc4pnpi4o5ognkhiwkkxnz_zg1ijpaqy.min.css");
}
@*<link href="https://pavilion.nop-templates.com/bundles/tzjkkql3ws3xc4pnpi4o5ognkhiwkkxnz_zg1ijpaqy.min.css" rel="stylesheet" />*@
@if (IsSectionDefined("Breadcrumb"))
{
    @section Breadcrumb {
        @RenderSection("Breadcrumb")
    }
}
<div class="center-1">
    @await Component.InvokeAsync("Widget", new { widgetZone = PublicWidgetZones.MainColumnBefore })
    @RenderBody()
    @await Component.InvokeAsync("Widget", new { widgetZone = PublicWidgetZones.MainColumnAfter })
</div>

